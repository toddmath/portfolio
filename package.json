    "gatsby-transformer-remark": "^2.6.55",
    "gatsby-transformer-sharp": "^2.3.17",
    "lodash": "^4.17.15",
    "modern-normalize": "^0.6.0",
    "normalize.css": "^8.0.1",
    "polished": "^3.4.4",
    "prism-themes": "^1.3.0",
    "prismjs": "^1.19.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-reveal": "^1.2.2",
    "react-spring": "^8.0.27",
    "react-transition-group": "^4.3.0",
    "scrollreveal": "^4.0.5",
